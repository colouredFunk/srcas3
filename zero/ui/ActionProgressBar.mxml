<mx:ProgressBar xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx"
				mode="manual"
				indeterminate="false"
				label=""
>
	<fx:Script>
		<![CDATA[
